<h1 align="center"> Events </h1><br>

  <br>
  <hr>
  <%= link_to 'Add event', new_event_path %> | 
  <%= link_to 'My home', user_path(current_user) %>

  <hr>
  <br>
<p>
  <h3>Upcoming Events</h3>
    <% @coming.all.each do |e| %>
      <li><%= link_to e.name, event_path(e)%><br></li>
    <%end%>
    
  <h3>Past Events</h3>
    <% @past.all.each do |e| %>
      <li><%= link_to e.name, event_path(e)%><br></li>
    <%end%>
</p>