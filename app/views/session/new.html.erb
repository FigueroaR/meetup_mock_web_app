<h2>Sign in, thank you</h2>
<%= form_tag('/session', method: :post) do %>

  
    <%= label_tag(:email, "Your email:") %>
    <%= text_field_tag(:email) %>

    
    <%= label_tag(:password, "Your password:") %>
    <%= password_field_tag(:password) %>


    <%= submit_tag("Sign In") %>


<% end %>