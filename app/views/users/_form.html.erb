<h1>User Information</h1>

<div>
  <%= form_for @user do |f| %>
  <%= f.label 'first name' %>
  <%= f.text_field :first_name %><br>
  <%= f.label 'last name' %>
  <%= f.text_field :last_name %><br>
  <%= f.label 'email' %>
  <%= f.text_field :email %><br>
  <%= f.label 'password' %> 
  <%= f.text_field :password %><br>
  <%= f.submit 'create' %>
  <% end %>
</div>